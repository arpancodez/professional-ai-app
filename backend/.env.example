# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Cloud Tunnel Configuration (for mobile HTTPS access)
# Add your cloud tunnel configuration here (e.g., ngrok, cloudflare tunnel, etc.)
TUNNEL_URL=your_https_tunnel_url_here

# CORS Settings
CORS_ORIGIN=*

# Rate Limiting (optional)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
